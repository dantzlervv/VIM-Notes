<!DOCTYPE html>
<html lang="en">
<head>
    <% include ../partials/head %>
</head>
<body class="container">

<header>
    <% include ../partials/header %>
</header>

<main>
    <div class="bg-light rounded" style="height: 80vh">
        <% lists.forEach(function(list) { %>
            <a href="/lists/edit/<%= list._id %>">
                <div>
                    <%= list.listName %>
                </div>
            </a>
        <% }); %>
    </div>
    <div class="bg-light rounded" style="height: 80vh">
        <% notes.forEach(function(note) { %>
            <a href="/notes/edit/<%= note._id %>">
                <div>
                    <%= note.noteTitle %>
                </div>
            </a>
        <% }); %>
    </div>
</main>

<footer>
    <% include ../partials/footer %>
</footer>

</body>
</html>
